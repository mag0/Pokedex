<div class="w3-container w3-content w3-center w3-padding-64" style="max-width:800px" id="band">
    <h2 class="w3-wide">Pokemones</h2>
    <table class="w3-table">
        <tr>
            <th>Imagen</th>
            <th>Tipo</th>
            <th>Numero</th>
            <th>Nombre</th>
        </tr>
        {{#pokemones}}
            <tr>
                <td><img style="width: 5em; height: 5em;" src="public/imagenes/pokemones/{{numero}}.png"></td>
                <td><img style="width: 5em; height: 1em;" src="public/imagenes/TipoPokemon/tipo_{{tipo}}.png"></td>
                <td>{{numero}}</td>
                <td>{{imagen}}</td>
            </tr>
        {{/pokemones}}
    </table>
    <a href="/tours/add">Nuevo tour</a>
</div>